{
  "name": "react-ts-vite-tailwindcss-template",
  "version": "1.0.0",
  "description": "App template using Vite, React, TypeScript and TailwindCSS",
  "author": "Ramniel Labaut <ramniel.labaut@gmail.com>",
  "license": "MIT",
  "repository": "https://github.com/ramniel/react-ts-vite-tailwind-template",
  "scripts": {
    "dev": "vite",
    "dev:tailwind": "npx tailwindcss@3.2.4 -i ./src/index.css -o ./src/output.css --watch",
    "build": "npm run build:tailwind && tsc && vite build",
    "build:tailwind": "npx tailwindcss@3.2.4 -i ./src/index.css -o ./src/output.css",
    "preview": "vite preview",
    "lint": "eslint --ignore-path .eslintignore --ext .js,.ts,.tsx .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|tsx|json)\""
  },
  "dependencies": {
    "@react-spring/parallax": "^9.7.2",
    "@types/react-router": "^5.1.20",
    "daisyui": "^2.14.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^4.8.0",
    "react-router-dom": "6.3.0",
    "react-spring": "^9.7.1",
    "react-tilt": "^1.0.2",
    "react-use-measure": "^2.1.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/history": "^5.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-react": "^1.3.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.2.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.13",
    "vite-tsconfig-paths": "^3.4.1"
  }
}
